@model IEnumerable<Prolab.Models.kullanıcı>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Yeni oluştur", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.retundate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Book.Bookname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Username)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.retundate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Book.Bookname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Username)
        </td>
        <td>
            @Html.ActionLink("Düzenle", "Edit", new { id=item.kullanıcıid }) |
            @Html.ActionLink("Detaylar", "Details", new { id=item.kullanıcıid }) |
            @Html.ActionLink("Sil", "Delete", new { id=item.kullanıcıid })
        </td>
    </tr>
}

</table>
